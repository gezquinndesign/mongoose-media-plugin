{
  "name": "mongoose-media-plugin",
  "version": "0.0.1",
  "description": "Mongoose plugin for handling media files in the cloud.",
  "main": "plugin.js",
  "scripts": {
    "test": "nyc mocha -r ts-node/register src/**/*.spec.ts"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "coverage/**/*"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/interactiverooster/mongoose-media-plugin.git"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "aws",
    "s3",
    "audio",
    "video",
    "ffmpeg",
    "media"
  ],
  "author": "Dan Bowles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/interactiverooster/mongoose-media-plugin/issues"
  },
  "homepage": "https://github.com/interactiverooster/mongoose-media-plugin#readme",
  "dependencies": {
    "@types/node": "^12.7.4",
    "aws-sdk": "^2.525.0",
    "typescript": "^3.6.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "coveralls": "^3.0.6",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "uuid": "^3.3.3"
  }
}
